{
  "name": "@mindcache/server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --local",
    "deploy": "wrangler deploy --env production",
    "typecheck": "tsc --noEmit",
    "test": "echo 'Integration tests require running server - run manually with: pnpm test:integration'",
    "test:integration": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "wrangler d1 migrations apply mindcache-db --env production --remote",
    "db:migrate:local": "wrangler d1 migrations apply mindcache-db --local",
    "db:migrations:list": "wrangler d1 migrations list mindcache-db --remote",
    "db:migrations:list:local": "wrangler d1 migrations list mindcache-db --local",
    "deploy:full": "./deploy.sh"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "wrangler": "^4.51.0",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.30",
    "@mindcache/shared": "workspace:*",
    "ai": "^5.0.45",
    "zod": "^3.23.0"
  }
}
