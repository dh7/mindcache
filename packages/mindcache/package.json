{
  "name": "mindcache",
  "version": "2.0.0-alpha.1",
  "description": "A TypeScript library for managing short-term memory in AI agents through an LLM-friendly key-value repository",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cloud": {
      "import": "./dist/cloud/index.mjs",
      "require": "./dist/cloud/index.js",
      "types": "./dist/cloud/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "llm",
    "memory",
    "agent",
    "typescript",
    "vercel-ai-sdk",
    "nextjs",
    "key-value",
    "storage",
    "context",
    "cloud",
    "real-time"
  ],
  "author": "MindCache Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dh7/mindcache.git"
  },
  "bugs": {
    "url": "https://github.com/dh7/mindcache/issues"
  },
  "homepage": "https://github.com/dh7/mindcache#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "ai": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

