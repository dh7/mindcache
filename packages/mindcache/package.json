{
  "name": "mindcache",
  "version": "3.4.4",
  "description": "A TypeScript library for managing short-term memory in AI agents through an LLM-friendly key-value repository",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./cloud": {
      "types": "./dist/cloud/index.d.ts",
      "import": "./dist/cloud/index.mjs",
      "require": "./dist/cloud/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "llm",
    "memory",
    "agent",
    "typescript",
    "vercel-ai-sdk",
    "nextjs",
    "key-value",
    "storage",
    "context",
    "cloud",
    "real-time"
  ],
  "author": "MindCache Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dh7/mindcache.git"
  },
  "bugs": {
    "url": "https://github.com/dh7/mindcache/issues"
  },
  "homepage": "https://github.com/dh7/mindcache#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "fast-diff": "^1.3.0",
    "lib0": "^0.2.115",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "^1.0.7",
    "yjs": "^13.6.28",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "ai": ">=3.0.0",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}